//prints out primes
repeat 100: (
    n = repetitions + 2;    //consider only numbers >= 2
    repeat n ** 0.5: (
        factor = repetitions + 2;   //consider only root n >= factor >= 2
        if n % factor == 0: break;
    )
    else:
        @n;
)